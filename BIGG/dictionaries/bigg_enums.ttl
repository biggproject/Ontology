bigg:TransportRelated.BusStation a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Bus station building";
  skos:broader bigg:TransportRelated .

bigg:TransportRelated.TrainStation a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Train station building";
  skos:broader bigg:TransportRelated .

bigg:TransportRelated.PortBuilding a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Port building";
  skos:broader bigg:TransportRelated .

bigg:TransportRelated.AirportBuilding a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Airport building";
