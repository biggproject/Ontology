  skos:broader bigg:Office .

bigg:Office.PrivateOffice a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Privetely owned structure where administrative work is performed";
  skos:broader bigg:Office .

bigg:Retail a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Wholesale and retail trade buildings" .

bigg:Retail.ShoppingMall a bigg:BuildingSpaceUseType, skos:Concept;
