  skos:broader bigg:Retail .

bigg:Retail.Supermarket a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Retailer supplying a wide range of products including groceries";
  skos:broader bigg:Retail .

bigg:Retail.Market a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Building for the gathering of people for the purchase and sale of provisions, livestock, and other commodities";
