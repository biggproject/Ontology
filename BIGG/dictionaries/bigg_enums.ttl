bigg:Other a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Building dedicated to other uses" .

bigg:Multi-use a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Building dedicated to multiple uses" .

bigg:Unknown a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Building which use or purpose is unknow to the user" .

