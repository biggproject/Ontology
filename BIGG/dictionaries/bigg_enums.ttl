bigg:Public.PoliceStation a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Headquarters of a police force";
  skos:broader bigg:Public .

bigg:Public.OtherPublicAuthorityFacilities a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Headquarters of other public authorities";
  skos:broader bigg:Public .

bigg:Public.Penitentiary a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Penitentiary or correctional facility";
  skos:broader bigg:Public .

bigg:Public.MunicipalHall a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Multi-use municipal space or hall";
  skos:broader bigg:Public .

bigg:Public.OtherPublic a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Any other public building or structure that does not belong to the previous categories";
  skos:broader bigg:Public .

