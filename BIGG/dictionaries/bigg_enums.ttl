bigg:Healthcare.DayCareHospital a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Premises that include urgent care centers and outpatient clinics";
  skos:broader bigg:Healthcare .

bigg:Healthcare.Hospital a bigg:BuildingSpaceUseType, skos:Concept;
  rdfs:comment "Premises that provide temporary to long-term inpatient services";
