                                   <http://purl.org/dc/terms/source> <https://www.etsi.org/deliver/etsi_ts/103400_103499/10341004/01.01.01_60/ts_10341004v010101p.pdf> ;
                                   <http://purl.org/vocab/vann/preferredNamespacePrefix> "s4city" ;
